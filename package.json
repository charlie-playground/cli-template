{
  "name": "cli-template",
  "version": "0.0.0",
  "type": "module",
  "license": "UNLICENSED",
  "private": true,
  "sideEffects": false,
  "main": "src/index.ts",
  "types": "src/index.ts",
  "bin": {
    "cli-template": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/src",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "bin": "cli-template",
    "dirname": "cli-template",
    "commands": "./src/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-not-found"
    ],
    "topicSeparator": " "
  },
  "scripts": {
    "ci": "bun run typecheck && bun run lint && bun run test",
    "dev": "bun run ./bin/dev.js",
    "clean": "rm -rf node_modules",
    "fix": "eslint --fix --quiet . ; prettier --write --log-level=silent .",
    "lint": "eslint . && prettier --check .",
    "prepare": "husky",
    "prepublishOnly": "bun run ci",
    "release": "np",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@dexaai/config/prettier",
  "dependencies": {
    "@inquirer/prompts": "^7",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-not-found": "^3",
    "@oclif/plugin-plugins": "^5",
    "dotenv": "^16",
    "ink": "^5",
    "ora": "^8"
  },
  "devDependencies": {
    "@dexaai/config": "latest",
    "@types/bun": "^1",
    "eslint": "^9",
    "husky": "^9",
    "lint-staged": "^15",
    "msw": "2.3.1",
    "np": "^10",
    "oclif": "^4",
    "prettier": "^3",
    "stdout-stderr": "^0.1.13",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,md,mdx}": [
      "prettier --write"
    ]
  },
  "bun-create": {
    "preinstall": "rm -rf bun.lock",
    "postinstall": "bun run build",
    "start": "./install.sh <new_package_name>"
  }
}
